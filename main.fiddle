(require '[rewrite-clj.zip :as z]
         '[clojure.string :as str]
         '[main :refer [replace-defs replace-locals f]])

(z/sexpr (z/of-string* (str/replace
                        (str
                         (replace-defs
                          (-> (str (list (replace-locals f)))
                              z/of-string)))
                        #"(\w+)__\d+" "$1")))